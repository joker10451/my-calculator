{
  "name": "schitay-ru-calculators",
  "private": true,
  "version": "1.0.0",
  "description": "Универсальный помощник для расчетов Считай.RU",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/generate-static-html.js && node scripts/generate-sitemap.js",
    "generate:static-html": "node scripts/generate-static-html.js",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:edge": "playwright test --project=edge",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:report": "playwright show-report",
    "generate:feeds": "node scripts/generate-feeds.js",
    "generate:spa-fallback": "node scripts/generate-spa-fallback.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:blog-placeholders": "node scripts/generate-blog-placeholders.js",
    "validate:favicon-headers": "node scripts/validateFaviconHeaders.js",
    "validate:favicon-headers:prod": "node scripts/validateFaviconHeaders.js https://schitay-online.ru",
    "audit:performance": "node scripts/performance-audit.js",
    "audit:load-time": "node scripts/measure-load-time.js",
    "audit:bundle": "node scripts/analyze-initial-bundle.js",
    "audit:accessibility": "node scripts/accessibility-audit.js",
    "audit:all": "npm run audit:load-time && npm run audit:bundle && npm run audit:accessibility",
    "optimize:bundle": "node scripts/optimize-bundle.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:all": "npm run optimize:bundle && npm run optimize:images",
    "backup:create": "node scripts/create-backup.js",
    "backup:restore": "node scripts/restore-backup.js",
    "deploy:prepare": "npm run build && npm run backup:create",
    "deploy:check": "npm run test && npm run audit:all",
    "production:check": "node scripts/check-production.js",
    "analytics:setup": "node scripts/setup-analytics.js",
    "analytics:dashboard": "node scripts/generate-analytics-dashboard.js",
    "seo:generate-urls": "node scripts/submit-to-search-engines.js",
    "analyze:blog-data": "node scripts/analyze-blog-data.js",
    "test:consolidation": "node scripts/test-consolidation.js",
    "consolidate:blog-data": "node scripts/consolidate-blog-data.js"
  },
